{
  "name": "@fe6/amap",
  "version": "0.0.0",
  "description": "Gaud components of vue3",
  "scripts": {
    "start": "vite",
    "build:lib": "vite build --config vite.config.lib.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "lint": "pretty-quick --staged",
    "prepublishOnly": "pnpm build:lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fe6/amap.git"
  },
  "keywords": [
    "amap"
  ],
  "author": "李梦龙",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fe6/amap/issues"
  },
  "homepage": "https://github.com/fe6/amap#readme",
  "engines": {
    "node": ">= 16.0.0",
    "pnpm": ">=6 <7"
  },
  "packageManager": "pnpm@6.32.2",
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "node_modules/@fe6/norm-git-cz-config/customizable.js"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@fe6/norm-git-cz-config": "^0.1.1",
    "@fe6/norm-prettier": "^0.1.0",
    "@fe6/water-pro": "^4.16.3",
    "@vitejs/plugin-vue": "^3.0.3",
    "cz-customizable": "^6.9.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "vitepress": "1.0.0-alpha.8",
    "vue": "^3.2.37",
    "vue-tsc": "^0.40.1"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/a-map.umd.cjs",
  "module": "./dist/a-map.js",
  "exports": {
    ".": {
      "import": "./dist/a-map.js",
      "require": "./dist/a-map.umd.cjs"
    }
  }
}
